{
  "manifest_version": 2,
  "name": "Ajanuw Test",
  "version": "0.02",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "images/ct.png",
    "default_title": "Google Mail",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"]
}
